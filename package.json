{
  "name": "kong-request-api-usage-tracker",
  "version": "0.1.0",
  "description": "KRAUT - a prometheus exporter for Kong API usage",
  "author": "",
  "private": true,
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "clean": "rm -rf lib",
    "lint": "eslint \"src/**/*.ts\"",
    "start": "node lib/index.js",
    "start:watch": "nodemon lib/index.js",
    "start:generator": "ts-node __tests__/samples/sample-generator.ts",
    "build": "yarn run lint && echo Using TypeScript && tsc --version && tsc --pretty -p .",
    "build:docker": "yarn run build && docker build -t apolloai/kraut:latest . && docker push apolloai/kraut:latest",
    "build:docker:production": "yarn run build && docker build -t apolloai/kraut:production . && docker push apolloai/kraut:production",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "watch": "yarn run build -- --watch",
    "watch:test": "yarn run test -- --watch"
  },
  "dependencies": {
    "fastify": "~2.14.1",
    "lodash": "^4.17.15",
    "node-fetch": "^2.6.0",
    "prom-client": "^12.0.0",
    "typescript-simple-di": "^1.1.4"
  },
  "devDependencies": {
    "@types/lodash": "~4.14.152",
    "@types/node": "~14.0.5",
    "@types/node-fetch": "~2.5.7",
    "@typescript-eslint/eslint-plugin": "~3.0.0",
    "@typescript-eslint/parser": "~3.0.0",
    "eslint": "~7.0.0",
    "eslint-config-prettier": "~6.11.0",
    "eslint-plugin-prettier": "~3.1.3",
    "jest": "~26.0.1",
    "nodemon": "~2.0.4",
    "prettier": "~2.0.5",
    "ts-jest": "~26.0.0",
    "ts-node": "~8.10.1",
    "typescript": "~3.9.3"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
